<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/anon"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="tiked.com.tiked.MainActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="msgHola"
        android:text="hola"
        android:layout_below="@+id/button5"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="logout"
        android:text="LO"
        android:layout_above="@+id/button5"
        android:layout_toStartOf="@+id/button3" />

    <Button
        android:id="@+id/button3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="poweroff"
        android:text="Off"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="killchrome"
        android:text="Kill chrome"
        android:layout_below="@+id/button5"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="killexplorer"
        android:text="Kill explorer"
        android:layout_below="@+id/button3"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="startexplorer"
        android:text="run explorer"
        android:layout_above="@+id/button5"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/trgt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="none"
        android:hint="target"
        android:layout_above="@+id/button13"
        android:layout_alignParentStart="true"
        android:text="*" />

    <EditText
        android:id="@+id/args"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:capitalize="none"
        android:hint="args"
        android:layout_above="@+id/cmdt"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <EditText
        android:id="@+id/cmdt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:capitalize="none"
        android:hint="cmd"
        android:layout_above="@+id/trgt"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/args" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:onClick="sendbtn"
        android:text="Send"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />


    <Button
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="errorBtn"
        android:text="Error"
        android:layout_alignBaseline="@+id/button12"
        android:layout_alignBottom="@+id/button12"
        android:layout_alignEnd="@+id/args" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Error Grave"
        android:id="@+id/button7"
        android:onClick="errorGraveBtn"
        android:layout_above="@+id/button8"
        android:layout_alignEnd="@+id/button14" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Error super grave"
        android:id="@+id/button8"
        android:onClick="errorSuperGrave"
        android:layout_below="@+id/button11"
        android:layout_alignParentEnd="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Youtube"
        android:onClick="YtBtn"
        android:id="@+id/button9"
        android:layout_below="@+id/button11"
        android:layout_alignParentStart="true" />

    <Switch
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10x  "
        android:id="@+id/switch1"
        android:layout_alignBottom="@+id/button10"
        android:layout_alignParentEnd="true"
        android:checked="false"
        android:onClick="x10sw" />


</RelativeLayout>
